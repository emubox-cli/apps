{
    "name": "Yuzu",
    "consoles": ["switch"],
    "gameExec": "-f -g {}",
    "romQuery": ".*(.kip|.KIP|.nca|.NCA|.nro|.NRO|.nso|.NSO|.nsp|.NSP|.xci|.XCI)",
    "postInstall": {
        "makeDirs": [
            "<config>/<id>",
            "<save>/<id>/nand",
            "<save>/<id>/sdmc"
        ],
        "makeFiles": [
            {
                "path": "<config>/<id>/qt-config.ini",
                "content": {
                    "Data%20Storage": {
                        "nand_directory": "<save>/<id>/nand",
                        "sdmc_directory": "<save>/<id>/sdmc"
                    }
                }
            }
        ]
    },
    "installOptions": {
        "manual": true
    }
}

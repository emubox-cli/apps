{
    "name": "Snes9x",
    "consoles": ["snes"],
    "gameExec": "{}",
    "romQuery": ".*(.smc|.SMC|.sfc|.SFC|.swc|.SWC)",
    "postInstall": {
        "makeDirs": [
            "<config>/snes9x",
            "<save>/snes9x/saves",
            "<save>/snes9x/states"
        ],
        "makeFiles": [
            {
                "path": "<config>/snes9x/snes9x.conf",
                "content": {
                    "Files": {
                        "SRAMDirectory": "<save>/snes9x/saves",
                        "SaveStateDirectory": "<save>/snes9x/states"
                    },
                    "Sound": {
                        "Driver": 3
                    }
                }
            }
        ]
    },
    "installOptions": {
        "flatpak": "com.snes9x.Snes9x",
        "flatpakOverrideFs": true,
        "aur": "snes9x-gtk-git",
        "aurExportName": "snes9x-gtk",
        "libretroCore": "snes9x",
        "gitRepo": "snes9xgit/snes9x",
        "gitRe": "Snes9x-.*-x86_64.AppImage"
    }
}

{
    "name": "Rosalie's Mupen GUI",
    "consoles": ["n64"],
    "gameExec": "-f {}",
    "romQuery": ".*(.n64|.N64|.z64|.Z64|.v64|.V64|.ndd|.NDD|.d64|.D64|.zip|.ZIP|.7z|.7Z)",
    "postInstall": {
        "makeLinks": {},
        "makeDirs": [
            "<config>/RMG",
            "<save>/rmg/saves",
            "<save>/rmg/states"
            
        ],
        "makeFiles": [{
            "path": "<config>/RMG/mupen64plus.cfg",
            "content": {
                "Core": {
                    "SaveSRAMPath": "\"<save>/rmg/saves\"",
                    "SavesStatePath": "\"<save>/rmg/states\""
                }
            }
        }]
    },
    "installOptions": {
        "flatpak": "com.github.Rosalie241.RMG",
        "aur": "rmg-git",
        "aurExportName": "RMG",
        "gitRepo": "rosalie241/rmg",
        "gitRe": "RMG-Portable-Linux64-v.*.AppImage"
    },
    "$schema": "_schema.json"
}